doctype html
html(lang='en')
    include includes/head
    body
        .loading-container
            .loading-circle.vetical-center
            span.vetical-center.number 0%
        .wrapper(style="display: none;")
            .swiper-container
                .swiper-wraper
                    include components/box0
                    include components/box1
                    include components/box2
                    include components/box3
                    include components/box4
                    include components/box5
                    include components/box6
                    include components/box7
                    include components/box8
                .swiper-pagination
        //- zepto
        script(src="js/zepto.min.js")
        //- swiper
        script(src="vendor/swiper/dist/js/swiper.jquery.min.js")
        script(src="js/swiper.animate.min.js")
        //- 引用微信js-sdk文件
        script(src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js")
        script(src="js/jweixin.js")
        script(src="js/bundle.js")
        //- 表单
        script(src="js/formData.js")