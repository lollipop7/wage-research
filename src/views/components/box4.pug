.swiper-slide.box4(data-hash="slide4")
    include ../includes/header
    img.page-num.ani(src="images/forms/4-5.png",swiper-animate-effect="slideInRight",swiper-animate-duration=".5s",swiper-animate-delay=".19s")
    ul.commited
        //问题10
        li.inner.ani(swiper-animate-effect="zoomIn",swiper-animate-duration=".5s",swiper-animate-delay=".29s")
            img(src="images/forms/question10.png", class="question10")
            .choice
                - var tenththList = [1,2,3,4,5,6]
                each item, index in tenththList
                    label.label-container
                        img(src=`images/forms/question10/${item}.png`)
                        input.nair-radio-input(type="radio" name="10", value=`${item}`)
                        span.nair-radio-inner
        //问题11
        li.inner.ani(swiper-animate-effect="zoomIn",swiper-animate-duration=".5s",swiper-animate-delay=".39s")
            .label-text
                img(src="images/forms/question11.png", class="question11")
                input(type="number", id="benefit")
    img.logo51jrq.justify-center.ani(src="images/forms/logo51jrq.png",swiper-animate-effect="slideInup",swiper-animate-duration=".5s",swiper-animate-delay="1.09s")
    .arrow-down.justify-center